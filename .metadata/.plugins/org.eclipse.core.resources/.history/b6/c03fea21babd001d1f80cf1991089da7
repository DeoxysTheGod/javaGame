package equipements.rings;

public class LordRing extends Ring {
	
	public LordRing() {
		super();
		this.name = "Anneau du Roi";
		this.bonusHp   = 40;
		this.bonusAtk  = 20;
		this.bonusDef  = 10;
		this.bonusLuck = 2;
	}

	public String getName() {
		return name;
	}

	public int getHp() {
		return hp;
	}

	public int getAtk() {
		return atk;
	}

	public int getDef() {
		return def;
	}

	public int getLuck() {
		return luck;
	}

	@Override
	public String toString() {
		return "LordRing[name=" + name + ", hp=" + hp + ", atk=" + atk + ", def=" + def + ", luck=" + luck + "]";
	}
	
	
}
