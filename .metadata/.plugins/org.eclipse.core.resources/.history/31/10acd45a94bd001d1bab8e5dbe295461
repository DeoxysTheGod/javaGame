package equipements;

public class Stuff {
	
	private int id;
	
	private String name;

	private int bonusHp   ;
	private int bonusAtk  ;
	private int bonusDef  ;
	private int bonusCc   ;
	private int bonusLuck ;
	
	private double dropRate;
	
	public Stuff() {
		this.name = "Rien";
		
		this.bonusHp   = 0 ;
		this.bonusAtk  = 0 ;
		this.bonusDef  = 0 ;
		this.bonusCc   = 0 ;
		this.bonusLuck = 0 ;
		
		this.dropRate = 0.0;
	}
	
}
